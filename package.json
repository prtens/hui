  "author": "prtens <hux@huonu.com>",
  "private": false,
  "scripts": {
    "dev": "webpack-dev-server --open --inline --hot --compress --history-api-fallback --port 8080 --config build/webpack.dev.config.js",
    "start": "npm run dev",
    "dist:style": "gulp --gulpfile build/build-style.js",
    "dist:dev": "webpack --config build/webpack.dist.dev.config.js",
    "dist:prod": "webpack --config build/webpack.dist.prod.config.js",
    "dist:locale": "webpack --config build/webpack.dist.locale.config.js",
    "dist": "npm run dist:style && npm run dist:dev && npm run dist:prod && npm run dist:locale",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit",
    "pub": "sh ./build/bin/release.sh"
  },
